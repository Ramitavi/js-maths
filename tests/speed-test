sum = 0;
iters = 10000;

for (i = 0; i < iters; i = i + 1) {
    sum = sum + i;
}

println("Sum of first " + iters + " integers = " + sum);

# Speed Update Before: iters=10000, time=~170ms
# Speed Update 1: iters=10000, time=~151ms (+12.6%)
# Speed Update 2: iters=10000, time=~140ms (+07.9%; total +21.4%)